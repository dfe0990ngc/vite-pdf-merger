{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/resources/"
        },
        {
            "src": "/images(.*)",
            "dest": "/public/images/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://vite-pdf-merger.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        
        "DB_CONNECTION": "pgsql",
        "DB_URL": "postgres://default:iVxgKq5UHE7Y@ep-wild-snow-a4v0zz3v-pooler.us-east-1.aws.neon.tech/verceldb?sslmode=require",
        "DB_HOST": "ep-wild-snow-a4v0zz3v-pooler.us-east-1.aws.neon.tech",
        "DB_PORT": "5432",
        "DB_DATABASE": "verceldb",
        "DB_USERNAME": "default",
        "DB_PASSWORD": "iVxgKq5UHE7Y"
    }
}
